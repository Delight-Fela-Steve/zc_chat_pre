  <% if (isLocal) { %>
  <script type="systemjs-importmap">
    {
      "imports": {
        "@zuri/root-config": "//localhost:9000/zuri-root-config.js",
        "@zuri/chat": "//localhost:8080/zuri-chat.js"
      }
    }
  </script>
  <% } else { %>
  <script type="systemjs-importmap">
    {
      "imports": {
        "@zuri/root-config": "/zuri-root-config.js",
        "@zuri/chat": "/zuri-chat.js"
      }
    }
  </script>
  <% } %>

